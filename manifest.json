{
    "name": "PostLit Token Login",
    "description": "signin to postLit with a user token",
    "version": "0.1.0",
    "version_name": "0.1.0",
    "manifest_version": 3,
    "icons": {
      "16": "/icons/icon16.png",
      "48": "/icons/icon48.png",
      "128": "/icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/icons/icon16.png",
        "48": "/icons/icon48.png",
        "128": "/icons/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://*.postlit.dev/*"],
        "js": ["content.js"]
      }
    ]
  }
  